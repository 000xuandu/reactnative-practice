{
  "name": "myproject",
  "version": "1.0.0",
  "android-version": "1.0.0",
  "ios-version": "1.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "update-config-ios-staging": "bundle exec fastlane ios update_config --env staging",
    "update-config-ios-prod": "bundle exec fastlane ios update_config --env production",
    "update-config-android-staging": "bundle exec fastlane android update_config --env staging",
    "update-config-android-prod": "bundle exec fastlane android update_config --env production",

    "android:staging": "yarn update-config-android-staging && ENVFILE=./fastlane/.env.staging npx react-native run-android",
    "android:prod": "yarn update-config-android-prod && ENVFILE=./fastlane/.env.production npx react-native run-android",
    "android:build:staging": "bundle exec fastlane android build_apk_staging --env staging",
    "android:build:prod": "bundle exec fastlane android build_apk_production --env production",
    "android:deploy:staging": "bundle exec fastlane android distribute_staging --env staging",
    "android:deploy:prod": "bundle exec fastlane android distribute_production --env production",

    "ios:staging": "yarn update-config-ios-staging && ENVFILE=./fastlane/.env.staging react-native run-ios",
    "ios:prod": "yarn update-config-ios-prod && ENVFILE=./fastlane/.env.production react-native run-ios"
  },
  "dependencies": {
    "@notifee/react-native": "^3.0.4",
    "@react-native-firebase/app": "^13.0.1",
    "@react-native-firebase/messaging": "^13.0.1",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.4",
    "d3-shape": "^3.0.1",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-change-icon": "^3.1.1",
    "react-native-config": "^1.4.5",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-pager-view": "^5.4.9",
    "react-native-reanimated": "^2.2.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-video": "^5.2.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.1",
    "reselect": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/d3-shape": "^3.0.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.30",
    "@types/react-native": "^0.65.7",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "17.0.1",
    "redux-devtools": "^3.7.0",
    "ts-jest": "^27.0.6",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
